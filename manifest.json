{
  "manifest_version": 2,
  "name": "Brightspace Quiz Review",
  "version": "1.0",

  "description": "Allows users to hide quiz answers on already submitted quizzes.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "include_globs": [
        "*://brightspace.*/*"
      ],
      "js": ["main.js"]
    }
  ]
}
